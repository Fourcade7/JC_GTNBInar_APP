<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:name=".di.MyApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.JC_GTNBInar_APP"
        tools:targetApi="31">
        <activity
            android:name=".presentation.user.groupopen.GroupOpenActivity"
            android:exported="false"
            android:label="@string/title_activity_group_open"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.user.plansopen.PlansOpenActivity"
            android:exported="false"
            android:label="@string/title_activity_plans_open"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.user.send.SendActivity"
            android:exported="true"
            android:label="@string/title_activity_send"
            android:theme="@style/Theme.JC_GTNBInar_APP" >

        </activity>
        <activity
            android:name=".presentation.user.chooseplan.ChooseActivity"
            android:exported="false"
            android:label="@string/title_activity_auth"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.user.login.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.JC_GTNBInar_APP">

        </activity>
        <activity
            android:name=".presentation.user.register.RegisterActivity"
            android:exported="true"
            android:label="@string/title_activity_register"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.admin.groupopenadmin.GroupOpenAdminActivity"
            android:exported="true"
            android:label="@string/title_activity_group_open"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.admin.plansopenadmin.PlansOpenAdminActivity"
            android:exported="true"
            android:label="@string/title_activity_plans_open"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity
            android:name=".presentation.admin.resendadmin.ReSendAdminActivity"
            android:exported="true"
            android:label="@string/title_activity_re_send_admin"
            android:theme="@style/Theme.JC_GTNBInar_APP" />
        <activity

            android:name=".presentation.user.home.HomeActivity"
            android:exported="true"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.JC_GTNBInar_APP">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.admin.homeadmin.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.JC_GTNBInar_APP" >

        </activity>
    </application>

</manifest>